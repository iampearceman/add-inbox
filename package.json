{
    "name": "add-inbox",
    "version": "0.1.28",
    "description": "CLI tool to add Novu Notifications Inbox to your React or Next.js project",
    "type": "commonjs",
    "main": "dist/src/cli/index.js",
    "bin": {
        "add-inbox": "dist/src/cli/index.js"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "start": "ts-node src/cli/index.ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "novu",
        "inbox",
        "notifications",
        "react",
        "nextjs",
        "cli"
    ],
    "author": "Novu",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "prompts": "^2.4.2"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "files": [
        "dist",
        "package.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "preferGlobal": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iampearceman/add-inbox.git"
    },
    "bugs": {
        "url": "https://github.com/iampearceman/add-inbox/issues"
    },
    "homepage": "https://github.com/iampearceman/add-inbox#readme",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/commander": "^2.12.0",
        "@types/prompts": "^2.4.9",
        "rimraf": "^5.0.10",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.0"
    }
}
