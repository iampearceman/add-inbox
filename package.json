{
    "name": "add-inbox",
    "version": "0.1.3",
    "description": "CLI tool to add Novu's Inbox component to your project with a single command",
    "main": "index.js",
    "bin": {
        "add-inbox": "index.js"
    },
    "preferGlobal": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "files": [
        "index.js",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "scripts": {
        "test": "echo \"No tests configured\" && exit 0",
        "prepublishOnly": "npm test",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "postversion": "git push && git push --tags"
    },
    "keywords": [
        "novu",
        "inbox",
        "notifications",
        "cli",
        "component",
        "react",
        "nextjs",
        "tailwind"
    ],
    "author": "Emil Pearce",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.2",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "conventional-changelog-cli": "^4.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iampearceman/add-inbox.git"
    },
    "bugs": {
        "url": "https://github.com/iampearceman/add-inbox/issues"
    },
    "homepage": "https://github.com/iampearceman/add-inbox#readme",
    "publishConfig": {
        "access": "public"
    }
}
